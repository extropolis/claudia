# Server Configuration
PORT=3001

# SAP AI Core Configuration
# Provider ID for SAP AI Core (usually 'sap-ai-core')
SAP_AICORE_PROVIDER_ID=sap-ai-core

# Model ID to use (e.g., 'anthropic--claude-4.5-sonnet', 'anthropic--claude-3.5-sonnet')
SAP_AICORE_MODEL_ID=anthropic--claude-4.5-sonnet

# SAP AI Core Authentication URL (if required by your setup)
SAP_AICORE_AUTH_URL=

# SAP AI Core Client ID (OAuth client ID)
SAP_AICORE_CLIENT_ID=

# SAP AI Core Client Secret (OAuth client secret)
SAP_AICORE_CLIENT_SECRET=

# SAP AI Core Resource Group (deployment resource group)
SAP_AICORE_RESOURCE_GROUP=

# SAP AI Core Base URL (API endpoint)
SAP_AICORE_BASE_URL=

# LLM Service Configuration (for SAP AI Proxy)
# URL of the LLM API endpoint
LLM_API_URL=http://localhost:3030/v1/chat/completions

# Model identifier for the LLM service
LLM_MODEL=claude-sonnet-4-5-20250929

# API key for LLM authentication
LLM_API_KEY=test-key-1

# Authentication Secrets
# Secret key for JWT access token signing
JWT_SECRET=your-secret-key-change-in-production

# Secret key for JWT refresh token signing
JWT_REFRESH_SECRET=your-refresh-secret-key-change-in-production

# OpenCode Configuration
# Model to use with OpenCode (format: provider--model)
OPENCODE_MODEL=anthropic--claude-4.5-sonnet
